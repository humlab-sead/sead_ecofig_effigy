{
  "name": "sead_ecofig_effegy",
  "version": "1.0.0",
  "description": "SEAD Ecofig Effigy in 3D",
  "main": "src/main.js",
  "scripts": {
    "clean:all": "npm cache clean && npm run rimraf -- public/*",
    "clean": "mkdirp public && rimraf public/*",
    "copy:data": "cp api/geo2.json public/",
    "build:cesium": "cpr node_modules/cesium/Build/Cesium public/Cesium public/",
    "build:models": "npm run compile:glb",
    "build:assets": "npm run build:models && npm run build:cesium && npm run copy:data",
    "build:dev": "npm run clean && npm run build:assets && webpack --config ./config/webpack.config.js --progress --profile --colors --display-error-details --display-cached",
    "build:release": "npm run clean && npm run build:assets && npm run compile:glb && webpack -p --define process.env.NODE_ENV='\"production\"' --config ./config/webpack.config.js --progress --profile --colors --display-error-details --display-cached",
    "dev:server": "webpack-dev-server --config ./config/webpack.config.js --display-error-details",
    "dev:build": "npm run build:dev",
    "test:build": "webpack --config ./config/webpack.test.config.js --display-error-details --watch",
    "test:server": "start http://localhost:8090/testBundle && webpack-dev-server --debug --config ./config/webpack.test.config.js --client-log-level info --hot --inline",
    "compile:glb": "rimraf public/assets/* && node ./src/compileGlb.js && cpr resources/assets public/assets/",
    "python:server": "cd public && python -m http.server --cgi 80",
    "node:server": "cd public && httpserver 80 localhost"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "cesium": "^1.29.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "circular-dependency-plugin": "^2.0.0",
    "cpr": "^2.0.1",
    "css-loader": "^0.26.1",
    "csvtojson": "^1.1.2",
    "es6-promise": "^4.0.5",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "gltf-pipeline": "0.1.0-alpha9",
    "html-webpack-plugin": "^2.26.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "jquery-ui": "^1.12.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.0.0",
    "obj2gltf": "^0.1.6",
    "protobufjs": "^6.3.1",
    "rimraf": "^2.5.4",
    "script": "^0.1.4",
    "semver": "^5.3.0",
    "sizzle": "^2.3.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {}
}
