{
  "name": "visead-pvs",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "clean:all": "npm cache clean && npm run rimraf -- node_modules public",
    "clean": "mkdirp public & rimraf public/*",
    "build:cesium": "cpr node_modules/cesium/Build/Cesium public/Cesium public/",
    "build:models": "cpr resources/Models/assets public/assets",
    "build:assets": "npm run build:models && npm run build:cesium",
    "build": "npm run clean && npm run build:assets && webpack --config webpack.config.js --progress --profile --colors --display-error-details --display-cached",
    "devserver": "webpack-dev-server --config ./config/webpack.config.js --display-error-details",
    "dev": "npm run build && npm run devserver",
    "test:build": "webpack --config ./config/webpack.test.config.js --display-error-details --watch",
    "test:server": "start http://localhost:8090/testBundle && webpack-dev-server --debug --config ./config/webpack.test.config.js --client-log-level info --hot --inline",
    "xxxrelease": "npm run build -- --optimize-minimize",
    "xxxtest": "echo \"Error: no test specified\" && exit 1",
    "xxxdll:build:cesium": "webpack --config ./webpack.cesium.dll.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "cesium": "^1.28.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "circular-dependency-plugin": "^2.0.0",
    "cpr": "^2.0.1",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.0.5",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "jquery-ui": "^1.12.1",
    "json-loader": "^0.5.4",
